@model PatientFilterVM

<div class="mt-3">
    <nav>
        <ul class="pagination">
            <li class="page-item"><a class="page-link" href="#">Previous</a></li>
            @for(int i = 1; i <= Model.PageCount; i++)
            {
                <li class="page-item">
                    <a class="page-link js-page-link" href="javascript:;" data-page="@i" >@i</a>
                </li>
            }
            <li class="page-item"><a class="page-link" href="#">Next</a></li>
        </ul>
    </nav>
</div>